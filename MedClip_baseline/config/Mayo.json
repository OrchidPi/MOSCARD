{
    "train_csv": "examples/train.csv",
    "dev_csv": "examples/test.csv",
    "backbone": "densenet121",
    "num_heads": 8,
    "width": 512,
    "height": 512,
    "long_side": 512,
    "fix_ratio": true,
    "pixel_mean": 128.0,
    "pixel_std": 64.0,
    "use_pixel_std": true,
    "use_equalizeHist": true,
    "use_transforms_type": "Aug",
    "gaussian_blur": 3,
    "border_pad": "pixel_mean",
    "num_classes": [1,1,1,1],
    "num_causal": [3,4],
    "num_causal2" :[1,1],
    "num_conf":[3,1],
    "query_idx": 0, 
    "batch_weight": true,
    "enhance_index": [2,6],
    "enhance_times": 1,
    "pos_weight": [1,1,1,1],
    "pos_weight_causal2": [1,1],
    "pos_weight_conf": [1],
    "train_batch_size":30,
    "dev_batch_size":30,
    "pretrained": true,
    "log_every": 10,
    "test_every": 100,
    "epoch": 20,
    "norm_type": "BatchNorm",
    "global_pool": "AVG_MAX",
    "fc_bn": true,
    "attention_map": "FPA",
    "lse_gamma": 0.5,
    "fc_drop": 0.0,
    "optimizer": "Adam",
    "criterion_target": "BCE",
    "criterion": "BCE", 
    "criterion_causal": "CE",
    "criterion_causal2": "BCE",
    "lr": 0.00001,  
    "lr_factor": 0.1,  
    "lr_epochs": [2,5],  
    "momentum": 0.85, 
    "weight_decay": 0.001,
    "best_target": "auc",
    "save_top_k": 3,
    "save_index": [0,1,2,3]
}